{
  "name": "suika-ui",
  "version": "1.0.0",
  "description": "Suika UI is a lightweight UI library for the Suika framework",
  "homepage": "https://jonwatkins.github.io/suika-ui/",
  "author": "Jon Watkins <jwatkins@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonWatkins/suika-ui.git"
  },
  "bugs": {
    "url": "https://github.com/JonWatkins/suika-ui/issues"
  },
  "main": "dist/bundle.umd.cjs",
  "browser": "dist/bundle.umd.cjs",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint": "npm run lint:format && npm run lint:fix ",
    "test": "jest",
    "production": "cross-env NODE_ENV=production vite build",
    "preview": "cross-env NODE_ENV=production vite preview",
    "build": "npm run lint && npm test && npm run production"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "ts-node": "^10.9.1",
    "vite": "^4.2.1"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "suika",
    "vdom",
    "JSX",
    "reactive"
  ]
}
